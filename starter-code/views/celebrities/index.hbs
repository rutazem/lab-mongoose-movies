<h2>
    All Celebs
</h2>

<ul>
    {{#each allCelebs}}

    <li>{{this.name}} //
        <a href="/celebrities/{{this.id}}">ID</a>
        <a href="/celebrities/{{this.id}}/edit">EDIT</a>



    </li>
    <form action="/celebrities/{{this.id}}/delete" method="POST">
        <button type="submit">DELETE BUTTON</button></form>
    {{/each}}
</ul>

<a href="/celebrities/new">Create a new one!</a>